{
    "version": 2,
    "builds": [
        {
            "src": "./src/system.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        { "handle": "filesystem" },
        { "src": "/public/(.*)", "dest": "/public/$1" },
        { "src": "/(.*)", "dest": "/system.js" }
    ]
}
