    <!-- Schema.org -->
    <meta itemprop="name" content="<%= title %>">
    <meta itemprop="description" content="IdolAPI - A Fanmade RESTful API based in IdolCorp">
    <!-- Twitter card -->
    <meta property="twitter:card" content="summary">
    <meta property="twitter:title" content="<%= title %>">
    <meta property="twitter:description" content="IdolAPI - A Fanmade RESTful API based in IdolCorp">

    <title><%= title %></title>
    <link rel="icon" href="/api/assets/favicon.ico" type="image/x-icon">

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Jost:wght@300;400;500&family=Manrope:wght@300;400;700&family=Readex+Pro:wght@300;400;600&display=swap" rel="stylesheet">
